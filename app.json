{
  "expo": {
    "name": "Hedwig",
    "slug": "hedwig-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "scheme": "hedwig",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hedwig.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSSupportsLiveActivities": true
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.hedwig.app",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.POST_NOTIFICATIONS",
        "android.permission.POST_PROMOTED_NOTIFICATIONS"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundleIdentifier": "com.hedwig.app"
    },
    "plugins": [
      "expo-router",
      "expo-web-browser",
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "16.0"
          },
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0",
            "minSdkVersion": 24,
            "newArchEnabled": true
          }
        }
      ],
      [
        "expo-live-activity",
        {
          "enablePushNotifications": true
        }
      ],
      "expo-font",
      "expo-secure-store",
      "expo-localization",
      [
        "@sentry/react-native/expo",
        {
          "organization": "hedwig",
          "project": "hedwig-app"
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "privyAppId": "",
      "privyClientId": "",
      "eas": {
        "build": {
          "experimental": {
            "ios": {
              "appExtensions": [
                {
                  "targetName": "LiveActivity",
                  "bundleIdentifier": "com.hedwig.app.LiveActivity",
                  "entitlements": {}
                }
              ]
            }
          }
        },
        "projectId": "5738569d-376f-4e3d-a2bb-b415156003dd"
      }
    },
    "owner": "hedwig-app"
  }
}